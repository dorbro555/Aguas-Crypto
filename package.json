    "passport": "^0.4.1",
    "passport-auth0": "^1.4.0",
    "rate-limit-redis": "^2.1.0",
    "technicalindicators": "^3.1.0",
    "tulind": "^0.8.18"
  },
  "devDependencies": {
    "concurrently": "^6.0.0"
